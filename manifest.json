{
    "name": "ChromeHeaderModifier",
    "version": "1.0",
    "description": "Modifies headers in chrome",
    "manifest_version": 2,
	"permissions":["storage","webRequest","webRequestBlocking","<all_urls>"],
	"background": {
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_popup":"popup.html"
	}
}